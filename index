    <script>
        let siteData = {
            homeNews: [],
            sets: [],
            pendingSets: [],
            users: [],
            contacts: {
                text: "–ù–∞–º–∏—Ä–∞–º–µ —Å–µ –≤ —Å—ä—Ä—Ü–µ—Ç–æ –Ω–∞ –¥–∏–≥–∏—Ç–∞–ª–Ω–∏—è —Ä–∞–π–æ–Ω –Ω–∞ –≥—Ä–∞–¥–∞.",
                address: "—É–ª. –°—Ä–µ–±—ä—Ä–Ω–∞ 21",
                phone: "02 / 910 00",
                email: "office@soundoura.bg"
            },
            listeners: 1284,
            streamUrl: "https://stream.radioveselina.bg:8000/veselina.mp3",
            likes: {}
        };
        
        let activePlayers = {};
        let currentEditingSetIndex = -1;
        let currentUser = null;
        let activeAlbumPlayer = null;
        
        let audioAnalysisData = null;
        
        function saveDataToStorage() {
            localStorage.setItem('soundouraData', JSON.stringify(siteData));
        }
        
        function loadDataFromStorage() {
            const savedData = localStorage.getItem('soundouraData');
            if (savedData) {
                const parsedData = JSON.parse(savedData);
                if (parsedData.homeNews) siteData.homeNews = parsedData.homeNews;
                if (parsedData.sets) siteData.sets = parsedData.sets;
                if (parsedData.pendingSets) siteData.pendingSets = parsedData.pendingSets;
                if (parsedData.users) siteData.users = parsedData.users;
                if (parsedData.contacts) siteData.contacts = parsedData.contacts;
                if (parsedData.listeners) siteData.listeners = parsedData.listeners;
                if (parsedData.streamUrl) siteData.streamUrl = parsedData.streamUrl;
                if (parsedData.likes) siteData.likes = parsedData.likes;
            }
        }
        
        function loadLikesFromStorage() {
            const savedLikes = localStorage.getItem('soundouraLikes');
            if (savedLikes) {
                siteData.likes = JSON.parse(savedLikes);
            }
        }
        
        function saveLikesToStorage() {
            localStorage.setItem('soundouraLikes', JSON.stringify(siteData.likes));
        }
        
        function isUserLoggedIn() {
            return currentUser !== null;
        }
        
        function loginUser(username, password) {
            const user = siteData.users.find(u => u.username === username && u.password === password);
            if (user) {
                currentUser = user;
                localStorage.setItem('currentUser', JSON.stringify(user));
                updateNavigationForLoggedInUser();
                return true;
            }
            return false;
        }
        
        function logoutCurrentUser() {
            currentUser = null;
            localStorage.removeItem('currentUser');
            updateNavigationForLoggedInUser();
            showSection('home');
            alert('–£—Å–ø–µ—à–Ω–æ –∏–∑–ª—è–∑–æ—Ö—Ç–µ –æ—Ç —Å–∏—Å—Ç–µ–º–∞—Ç–∞!');
        }
        
        function checkLoggedInUser() {
            const savedUser = localStorage.getItem('currentUser');
            if (savedUser) {
                currentUser = JSON.parse(savedUser);
                updateNavigationForLoggedInUser();
            }
        }
        
        function updateNavigationForLoggedInUser() {
            const profileNavButton = document.getElementById('profileNavButton');
            const navUserName = document.getElementById('navUserName');
            
            if (isUserLoggedIn() && profileNavButton && navUserName) {
                profileNavButton.style.display = 'flex';
                navUserName.textContent = currentUser.username;
                
                const registrationLink = document.querySelector('nav a[data-section="registration"]');
                if (registrationLink) {
                    registrationLink.textContent = '–ü—Ä–æ—Ñ–∏–ª';
                    registrationLink.setAttribute('data-section', 'profile');
                    registrationLink.setAttribute('onclick', 'showSection("profile")');
                }
            } else if (profileNavButton) {
                profileNavButton.style.display = 'none';
                
                const registrationLink = document.querySelector('nav a[data-section="profile"]');
                if (registrationLink) {
                    registrationLink.textContent = '–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è';
                    registrationLink.setAttribute('data-section', 'registration');
                    registrationLink.setAttribute('onclick', 'showSection("registration")');
                }
            }
        }
        
        function openProfileModal() {
            if (!isUserLoggedIn()) {
                showLoginForm();
                return;
            }
            
            const modal = document.getElementById('profileModal');
            const userName = document.getElementById('profileUserName');
            const userRole = document.getElementById('profileUserRole');
            const profileStats = document.getElementById('profileStats');
            
            if (userName) userName.textContent = currentUser.username;
            if (userRole) userRole.textContent = currentUser.role || '–ü–æ—Ç—Ä–µ–±–∏—Ç–µ–ª';
            
            const userSets = siteData.pendingSets.filter(set => set.dj === currentUser.username);
            const approvedSets = siteData.sets.filter(set => set.dj === currentUser.username);
            
            const statsHTML = `
                <div class="stat-item">
                    <div class="stat-value">${userSets.length}</div>
                    <div class="stat-label">–ò–∑–ø—Ä–∞—Ç–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ</div>
                </div>
                <div class="stat-item">
                    <div class="stat-value">${approvedSets.length}</div>
                    <div class="stat-label">–û–¥–æ–±—Ä–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ</div>
                </div>
                <div class="stat-item">
                    <div class="stat-value">${currentUser.likes || 0}</div>
                    <div class="stat-label">–õ–∞–π–∫–æ–≤–µ</div>
                </div>
                <div class="stat-item">
                    <div class="stat-value">${new Date(currentUser.registrationDate).toLocaleDateString('bg-BG')}</div>
                    <div class="stat-label">–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è</div>
                </div>
            `;
            
            if (profileStats) profileStats.innerHTML = statsHTML;
            modal.classList.add('active');
        }
        
        function closeProfileModal() {
            document.getElementById('profileModal').classList.remove('active');
            document.getElementById('userSetsContainer').style.display = 'none';
        }
        
        function viewUserSets() {
            const userSetsContainer = document.getElementById('userSetsContainer');
            const userSetsList = document.getElementById('userSetsList');
            
            if (!isUserLoggedIn()) return;
            
            const userSets = siteData.pendingSets.filter(set => set.dj === currentUser.username);
            
            if (userSets.length === 0) {
                userSetsList.innerHTML = '<p style="text-align:center; color:rgba(255,255,255,0.5); padding:20px;">–í—Å–µ –æ—â–µ –Ω—è–º–∞—Ç–µ –∏–∑–ø—Ä–∞—Ç–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ.</p>';
            } else {
                let setsHTML = '';
                userSets.forEach(set => {
                    setsHTML += `
                        <div style="background:rgba(255,255,255,0.02); border-radius:10px; border:1px solid rgba(255,0,255,0.3); padding:15px; margin-bottom:10px;">
                            <div style="display:flex; justify-content:space-between; align-items:center; margin-bottom:10px;">
                                <div style="font-weight:700; color:#fff;">${set.title}</div>
                                <div style="font-size:0.8rem; color:rgba(255,255,255,0.5);">${set.date}</div>
                            </div>
                            <div style="color:rgba(255,255,255,0.7); font-size:0.9rem;">
                                <p><strong>–°—Ç–∏–ª:</strong> ${set.style}</p>
                                <p><strong>–°—Ç–∞—Ç—É—Å:</strong> <span style="color:#ffcc00;">–ß–∞–∫–∞ –æ–¥–æ–±—Ä–µ–Ω–∏–µ</span></p>
                                ${set.description ? `<p><strong>–û–ø–∏—Å–∞–Ω–∏–µ:</strong> ${set.description}</p>` : ''}
                            </div>
                        </div>
                    `;
                });
                userSetsList.innerHTML = setsHTML;
            }
            
            userSetsContainer.style.display = 'block';
        }
        
        function editUserProfile() {
            if (!isUserLoggedIn()) return;
            
            const newPassword = prompt('–í—ä–≤–µ–¥–µ—Ç–µ –Ω–æ–≤–∞ –ø–∞—Ä–æ–ª–∞ (–æ—Å—Ç–∞–≤–µ—Ç–µ –ø—Ä–∞–∑–Ω–æ, –∞–∫–æ –Ω–µ –∏—Å–∫–∞—Ç–µ –¥–∞ —è –ø—Ä–æ–º–µ–Ω—è—Ç–µ):');
            if (newPassword === null) return;
            
            if (newPassword && newPassword.length < 6) {
                alert('–ü–∞—Ä–æ–ª–∞—Ç–∞ —Ç—Ä—è–±–≤–∞ –¥–∞ –±—ä–¥–µ –ø–æ–Ω–µ 6 —Å–∏–º–≤–æ–ª–∞!');
                return;
            }
            
            if (newPassword) {
                currentUser.password = newPassword;
                const userIndex = siteData.users.findIndex(u => u.id === currentUser.id);
                if (userIndex !== -1) {
                    siteData.users[userIndex] = { ...currentUser };
                }
                localStorage.setItem('currentUser', JSON.stringify(currentUser));
                saveDataToStorage();
                alert('–ü–∞—Ä–æ–ª–∞—Ç–∞ –µ –ø—Ä–æ–º–µ–Ω–µ–Ω–∞ —É—Å–ø–µ—à–Ω–æ!');
            }
        }
        
        function showUserProfilePage() {
            if (!isUserLoggedIn()) {
                showLoginForm();
                return;
            }
            
            const profilePageName = document.getElementById('profilePageName');
            const profilePageRole = document.getElementById('profilePageRole');
            const profilePageStats = document.getElementById('profilePageStats');
            
            if (profilePageName) profilePageName.textContent = currentUser.username;
            if (profilePageRole) profilePageRole.textContent = currentUser.role || '–ü–æ—Ç—Ä–µ–±–∏—Ç–µ–ª';
            
            const userSets = siteData.pendingSets.filter(set => set.dj === currentUser.username);
            const approvedSets = siteData.sets.filter(set => set.dj === currentUser.username);
            
            const statsHTML = `
                <div class="stat-item">
                    <div class="stat-value">${userSets.length}</div>
                    <div class="stat-label">–ò–∑–ø—Ä–∞—Ç–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ</div>
                </div>
                <div class="stat-item">
                    <div class="stat-value">${approvedSets.length}</div>
                    <div class="stat-label">–û–¥–æ–±—Ä–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ</div>
                </div>
                <div class="stat-item">
                    <div class="stat-value">${currentUser.likes || 0}</div>
                    <div class="stat-label">–õ–∞–π–∫–æ–≤–µ</div>
                </div>
                <div class="stat-item">
                    <div class="stat-value">${new Date(currentUser.registrationDate).toLocaleDateString('bg-BG')}</div>
                    <div class="stat-label">–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è</div>
                </div>
            `;
            
            if (profilePageStats) profilePageStats.innerHTML = statsHTML;
            loadUserSetsPage();
        }
        
        function loadUserSetsPage() {
            const userSetsPageList = document.getElementById('userSetsPageList');
            
            if (!isUserLoggedIn() || !userSetsPageList) return;
            
            const userSets = siteData.pendingSets.filter(set => set.dj === currentUser.username);
            
            if (userSets.length === 0) {
                userSetsPageList.innerHTML = '<p style="text-align:center; color:rgba(255,255,255,0.5); padding:20px;">–í—Å–µ –æ—â–µ –Ω—è–º–∞—Ç–µ –∏–∑–ø—Ä–∞—Ç–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ.</p>';
            } else {
                let setsHTML = '';
                userSets.forEach(set => {
                    setsHTML += `
                        <div style="background:rgba(255,255,255,0.02); border-radius:10px; border:1px solid rgba(255,0,255,0.3); padding:15px; margin-bottom:10px;">
                            <div style="display:flex; justify-content:space-between; align-items:center; margin-bottom:10px;">
                                <div style="font-weight:700; color:#fff;">${set.title}</div>
                                <div style="font-size:0.8rem; color:rgba(255,255,255,0.5);">${set.date}</div>
                            </div>
                            <div style="color:rgba(255,255,255,0.7); font-size:0.9rem;">
                                <p><strong>–°—Ç–∏–ª:</strong> ${set.style}</p>
                                <p><strong>–°—Ç–∞—Ç—É—Å:</strong> <span style="color:#ffcc00;">–ß–∞–∫–∞ –æ–¥–æ–±—Ä–µ–Ω–∏–µ</span></p>
                                ${set.description ? `<p><strong>–û–ø–∏—Å–∞–Ω–∏–µ:</strong> ${set.description}</p>` : ''}
                            </div>
                        </div>
                    `;
                });
                userSetsPageList.innerHTML = setsHTML;
            }
        }
        
        function editUserProfilePage() {
            editUserProfile();
        }
        
        function viewUserSetsPage() {
            const userSetsPageContainer = document.getElementById('userSetsPageContainer');
            if (userSetsPageContainer) {
                userSetsPageContainer.style.display = 'block';
                loadUserSetsPage();
            }
        }
        
        function showSection(sectionId) {
            const sections = document.querySelectorAll('.content-section');
            sections.forEach(section => {
                section.classList.remove('active');
            });
            
            const targetSection = document.getElementById(sectionId);
            if (targetSection) {
                targetSection.classList.add('active');
            }
            
            const links = document.querySelectorAll('.nav-link');
            links.forEach(link => {
                link.classList.remove('active');
                if (link.getAttribute('data-section') === sectionId) {
                    link.classList.add('active');
                }
            });
            
            if (sectionId === 'sets') {
                loadCleanSets();
            }
            
            if (sectionId === 'profile') {
                showUserProfilePage();
            }
            
            window.scrollTo({ top: 0, behavior: 'smooth' });
        }
        
        function initializeSite() {
            loadDataFromStorage();
            loadLikesFromStorage();
            checkLoggedInUser();
            
            document.getElementById('publicListenerCount').textContent = siteData.listeners.toLocaleString();
            document.getElementById('trackName').textContent = "SOUNDOURA –û–ù–õ–ê–ô–ù";
            document.getElementById('radioStream').src = siteData.streamUrl;
            
            loadLatestAlbums();
            checkSetUploadAccess();
        }
        
        function loadLatestAlbums() {
            const albumsContainer = document.getElementById('latestAlbums');
            
            if (!albumsContainer) return;
            
            const latestSets = siteData.sets.slice(0, 6);
            
            albumsContainer.innerHTML = '';
            
            if (latestSets.length === 0) {
                albumsContainer.innerHTML = `
                    <div class="no-albums-message">
                        <i class="fas fa-music"></i>
                        <p>–í—Å–µ –æ—â–µ –Ω—è–º–∞ –¥–æ–±–∞–≤–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ –≤ –∞—Ä—Ö–∏–≤–∞.</p>
                        <p style="font-size:0.9rem; margin-top:10px;">–ë—ä–¥–µ—Ç–µ –ø—ä—Ä–≤–∏—è—Ç, –∫–æ–π—Ç–æ —â–µ —Å–ø–æ–¥–µ–ª–∏ —Å–≤–æ—è —Å–µ—Ç!</p>
                    </div>
                `;
                return;
            }
            
            latestSets.forEach(set => {
                const albumCard = document.createElement('div');
                albumCard.className = 'album-card';
                albumCard.id = `album-card-${set.id}`;
                
                let description = set.description || '';
                if (description.length > 100) {
                    description = description.substring(0, 100) + '...';
                }
                
                const canDownload = isUserLoggedIn();
                const likeCount = siteData.likes[set.id] || 0;
                
                albumCard.innerHTML = `
                    <div class="album-date">${set.date}</div>
                    <div class="album-cover" onclick="toggleAlbumPlayer(${set.id})">
                        <div class="logo-text">SOUNDOURA</div>
                    </div>
                    <div class="album-info">
                        <div class="album-title">${set.title}</div>
                        <div class="album-dj">${set.dj}</div>
                        <div class="album-meta">
                            <span><i class="fas fa-music"></i> ${set.style}</span>
                            <span><i class="fas fa-clock"></i> ${set.duration} –º–∏–Ω</span>
                            <span><i class="fas fa-heart"></i> ${likeCount}</span>
                        </div>
                        ${description ? `<div class="album-description">${description}</div>` : ''}
                        
                        <div class="album-player" id="album-player-${set.id}">
                            <div class="mini-player-controls">
                                <button class="mini-play-btn" onclick="toggleAlbumPlay(${set.id})" id="album-play-btn-${set.id}">
                                    <i class="fas fa-play"></i>
                                </button>
                                <div class="mini-track-info">
                                    <div class="mini-track-title">${set.title}</div>
                                    <div class="mini-track-dj">${set.dj}</div>
                                </div>
                                <div class="mini-volume-control">
                                    <i class="fas fa-volume-low mini-volume-icon"></i>
                                    <input type="range" class="mini-volume-slider" min="0" max="100" value="70" oninput="changeAlbumVolume(${set.id}, this.value)" id="album-volume-${set.id}">
                                </div>
                            </div>
                            <div class="mini-progress-container">
                                <div class="mini-progress-bar" onclick="seekAlbumAudio(${set.id}, event)" id="album-progress-bar-${set.id}">
                                    <div class="mini-progress-fill" id="album-progress-fill-${set.id}"></div>
                                </div>
                                <div class="mini-progress-time">
                                    <span id="album-current-time-${set.id}">0:00</span>
                                    <span id="album-duration-${set.id}">0:00</span>
                                </div>
                            </div>
                            <audio id="album-audio-${set.id}" src="${set.url}" style="display:none" 
                                ontimeupdate="updateAlbumProgress(${set.id}, this.currentTime, this.duration)" 
                                onloadedmetadata="initAlbumDuration(${set.id}, this.duration)" 
                                onended="onAlbumAudioEnded(${set.id})">
                            </audio>
                        </div>
                        
                        <div class="album-actions">
                            <button class="album-btn play-main" onclick="toggleAlbumPlayer(${set.id})">
                                <i class="fas fa-play"></i> –ü—É—Å–Ω–∏
                            </button>
                            <button class="album-btn" onclick="viewAlbum(${set.id})" ${canDownload ? '' : 'disabled'}>
                                <i class="fas fa-download"></i> –°–≤–∞–ª–∏
                            </button>
                        </div>
                    </div>
                `;
                
                albumsContainer.appendChild(albumCard);
            });
        }
        
        function toggleAlbumPlayer(setId) {
            const playerElement = document.getElementById(`album-player-${setId}`);
            const albumCard = document.getElementById(`album-card-${setId}`);
            
            if (activeAlbumPlayer && activeAlbumPlayer !== setId) {
                const activePlayerElement = document.getElementById(`album-player-${activeAlbumPlayer}`);
                if (activePlayerElement) {
                    activePlayerElement.classList.remove('active');
                    
                    const activeAudio = document.getElementById(`album-audio-${activeAlbumPlayer}`);
                    if (activeAudio) {
                        activeAudio.pause();
                        const playBtn = document.getElementById(`album-play-btn-${activeAlbumPlayer}`);
                        if (playBtn) {
                            playBtn.innerHTML = '<i class="fas fa-play"></i>';
                        }
                    }
                }
            }
            
            if (playerElement.classList.contains('active')) {
                playerElement.classList.remove('active');
                activeAlbumPlayer = null;
                
                const audio = document.getElementById(`album-audio-${setId}`);
                if (audio) {
                    audio.pause();
                    const playBtn = document.getElementById(`album-play-btn-${setId}`);
                    if (playBtn) {
                        playBtn.innerHTML = '<i class="fas fa-play"></i>';
                    }
                }
            } else {
                playerElement.classList.add('active');
                activeAlbumPlayer = setId;
                
                albumCard.style.transform = 'translateY(-5px)';
                setTimeout(() => {
                    albumCard.style.transform = '';
                }, 300);
            }
        }
        
        function toggleAlbumPlay(setId) {
            const audio = document.getElementById(`album-audio-${setId}`);
            const playBtn = document.getElementById(`album-play-btn-${setId}`);
            
            if (!audio) return;
            
            stopAllAlbumAudio(setId);
            
            if (audio.paused) {
                audio.play();
                playBtn.innerHTML = '<i class="fas fa-pause"></i>';
                playBtn.style.background = 'rgba(255,0,255,0.3)';
            } else {
                audio.pause();
                playBtn.innerHTML = '<i class="fas fa-play"></i>';
                playBtn.style.background = 'rgba(255,0,255,0.2)';
            }
        }
        
        function stopAllAlbumAudio(currentSetId) {
            siteData.sets.slice(0, 6).forEach(set => {
                if (set.id !== currentSetId) {
                    const audio = document.getElementById(`album-audio-${set.id}`);
                    if (audio) {
                        audio.pause();
                        const playBtn = document.getElementById(`album-play-btn-${set.id}`);
                        if (playBtn) {
                            playBtn.innerHTML = '<i class="fas fa-play"></i>';
                            playBtn.style.background = 'rgba(255,0,255,0.2)';
                        }
                    }
                }
            });
        }
        
        function changeAlbumVolume(setId, value) {
            const audio = document.getElementById(`album-audio-${setId}`);
            if (audio) {
                audio.volume = value / 100;
            }
        }
        
        function updateAlbumProgress(setId, currentTime, duration) {
            const progressFill = document.getElementById(`album-progress-fill-${setId}`);
            const currentTimeElement = document.getElementById(`album-current-time-${setId}`);
            
            if (duration && duration > 0) {
                const progressPercent = (currentTime / duration) * 100;
                if (progressFill) {
                    progressFill.style.width = progressPercent + '%';
                }
                if (currentTimeElement) {
                    currentTimeElement.textContent = formatTime(currentTime);
                }
            }
        }
        
        function initAlbumDuration(setId, duration) {
            const durationElement = document.getElementById(`album-duration-${setId}`);
            if (durationElement && duration) {
                durationElement.textContent = formatTime(duration);
            }
        }
        
        function onAlbumAudioEnded(setId) {
            const playBtn = document.getElementById(`album-play-btn-${setId}`);
            if (playBtn) {
                playBtn.innerHTML = '<i class="fas fa-play"></i>';
                playBtn.style.background = 'rgba(255,0,255,0.2)';
            }
        }
        
        function seekAlbumAudio(setId, event) {
            const audio = document.getElementById(`album-audio-${setId}`);
            const progressBar = event.currentTarget;
            const rect = progressBar.getBoundingClientRect();
            const clickX = event.clientX - rect.left;
            const percent = clickX / rect.width;
            
            if (audio && audio.duration) {
                audio.currentTime = percent * audio.duration;
                updateAlbumProgress(setId, audio.currentTime, audio.duration);
            }
        }
        
        function viewAlbum(setId) {
            if (!isUserLoggedIn()) {
                alert('–¢—Ä—è–±–≤–∞ –¥–∞ —Å—Ç–µ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏, –∑–∞ –¥–∞ —Å–≤–∞–ª—è—Ç–µ —Å–µ—Ç–æ–≤–µ—Ç–µ!');
                return;
            }
            
            downloadSet(setId);
        }
        
        function checkSetUploadAccess() {
        }
        
        function formatTime(seconds) {
            if (isNaN(seconds)) return "0:00";
            const mins = Math.floor(seconds / 60);
            const secs = Math.floor(seconds % 60);
            return `${mins}:${secs < 10 ? '0' : ''}${secs}`;
        }
        
        // –§–£–ù–ö–¶–ò–ò –ó–ê –ß–ò–°–¢–ò–Ø –ü–õ–ï–ô–™–† –í –°–ï–ö–¶–ò–Ø "–°–ï–¢–û–í–ï"
        function loadCleanSets() {
            const cleanContainer = document.getElementById('setsCleanContainer');
            cleanContainer.innerHTML = '';
            
            if (siteData.sets.length === 0) {
                cleanContainer.innerHTML = `
                    <div class="no-sets-message">
                        <i class="fas fa-music" style="font-size:3rem; color:var(--primary-neon); margin-bottom:20px;"></i>
                        <p>–í—Å–µ –æ—â–µ –Ω—è–º–∞ –∫–∞—á–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ –≤ –∞—Ä—Ö–∏–≤–∞.</p>
                        <p style="font-size:0.9rem; margin-top:10px;">–ë—ä–¥–µ—Ç–µ –ø—ä—Ä–≤–∏—è—Ç, –∫–æ–π—Ç–æ —â–µ —Å–ø–æ–¥–µ–ª–∏ —Å–≤–æ—è —Å–µ—Ç!</p>
                    </div>
                `;
                return;
            }
            
            siteData.sets.forEach(set => {
                const likeCount = siteData.likes[set.id] || 0;
                const isLiked = localStorage.getItem(`liked_${set.id}`) === 'true';
                const canDownload = isUserLoggedIn();
                const audioId = `clean-audio-${set.id}`;
                
                const cleanPlayer = document.createElement('div');
                cleanPlayer.className = 'set-player-clean';
                cleanPlayer.id = `clean-player-${set.id}`;
                
                cleanPlayer.innerHTML = `
                    <div class="set-player-clean-header">
                        <div style="flex:1;">
                            <div class="set-player-clean-title">${set.title}</div>
                            <div class="set-player-clean-dj">${set.dj}</div>
                        </div>
                        <div class="set-player-clean-date">${set.date}</div>
                    </div>
                    
                    <div class="set-player-clean-meta">
                        <div><i class="fas fa-music"></i> ${set.style}</div>
                        <div><i class="fas fa-clock"></i> ${set.duration} –º–∏–Ω</div>
                        <div><i class="fas fa-heart"></i> ${likeCount} —Ö–∞—Ä–µ—Å–≤–∞–Ω–∏—è</div>
                        ${set.fileSize ? `<div><i class="fas fa-hdd"></i> ${set.fileSize}</div>` : ''}
                    </div>
                    
                    ${set.description ? `
                        <div class="set-player-clean-description">
                            <strong><i class="fas fa-info-circle"></i> –û–ø–∏—Å–∞–Ω–∏–µ:</strong> ${set.description}
                        </div>
                    ` : ''}
                    
                    <div class="clean-player">
                        <div class="clean-player-controls">
                            <button class="clean-play-btn" onclick="toggleCleanPlay('${audioId}', ${set.id})" id="clean-play-btn-${set.id}">
                                <i class="fas fa-play"></i>
                            </button>
                            <div class="clean-track-info">
                                <div class="clean-track-title">${set.title}</div>
                                <div class="clean-track-meta">
                                    <span><i class="fas fa-user"></i> ${set.dj}</span>
                                    <span><i class="fas fa-music"></i> ${set.style}</span>
                                    <span><i class="fas fa-clock"></i> ${set.duration} –º–∏–Ω</span>
                                </div>
                            </div>
                            <div class="clean-volume-control">
                                <i class="fas fa-volume-low" style="color:rgba(255,255,255,0.5); font-size:0.9rem;"></i>
                                <input type="range" class="clean-volume-slider" min="0" max="100" value="70" oninput="changeCleanVolume('${audioId}', this.value)" id="clean-volume-${set.id}">
                            </div>
                        </div>
                        
                        <div class="clean-progress-container">
                            <div class="clean-progress-bar" onclick="seekCleanAudio('${audioId}', ${set.id}, event)" id="clean-progress-bar-${set.id}">
                                <div class="clean-progress-fill" id="clean-progress-fill-${set.id}"></div>
                            </div>
                            <div class="clean-progress-time">
                                <span id="clean-current-time-${set.id}">0:00</span>
                                <span id="clean-duration-${set.id}">0:00</span>
                            </div>
                        </div>
                        
                        <div class="clean-actions">
                            <button class="clean-action-btn like ${isLiked ? 'liked' : ''}" onclick="cleanLikeSet(${set.id})" id="clean-like-btn-${set.id}">
                                <i class="${isLiked ? 'fas' : 'far'} fa-heart"></i> –•–∞—Ä–µ—Å–∞–π <span class="clean-like-count">${likeCount}</span>
                            </button>
                            <button class="clean-action-btn download" onclick="cleanDownloadSet(${set.id})" ${canDownload ? '' : 'disabled'} id="clean-download-btn-${set.id}">
                                <i class="fas fa-download"></i> –°–≤–∞–ª–∏
                            </button>
                        </div>
                        
                        <audio id="${audioId}" src="${set.url}" style="display:none" 
                            ontimeupdate="updateCleanProgress(${set.id}, this.currentTime, this.duration)" 
                            onloadedmetadata="initCleanDuration(${set.id}, this.duration)" 
                            onended="onCleanAudioEnded(${set.id})">
                        </audio>
                    </div>
                    
                    ${!canDownload ? '<div class="registration-required"><i class="fas fa-exclamation-circle"></i> –¢—Ä—è–±–≤–∞ –¥–∞ —Å—Ç–µ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏, –∑–∞ –¥–∞ —Å–≤–∞–ª—è—Ç–µ —Å–µ—Ç–æ–≤–µ—Ç–µ!</div>' : ''}
                `;
                
                cleanContainer.appendChild(cleanPlayer);
            });
        }
        
        function toggleCleanPlay(audioId, setId) {
            const audio = document.getElementById(audioId);
            const playBtn = document.getElementById(`clean-play-btn-${setId}`);
            
            if (!audio) return;
            
            stopAllCleanAudio(audioId);
            
            if (audio.paused) {
                audio.play();
                playBtn.innerHTML = '<i class="fas fa-pause"></i>';
                playBtn.style.background = 'var(--gradient-secondary)';
            } else {
                audio.pause();
                playBtn.innerHTML = '<i class="fas fa-play"></i>';
                playBtn.style.background = 'var(--gradient-primary)';
            }
        }
        
        function stopAllCleanAudio(currentAudioId) {
            const allAudioElements = document.querySelectorAll('.clean-player audio');
            allAudioElements.forEach(audioElement => {
                if (audioElement.id !== currentAudioId) {
                    const audio = audioElement;
                    audio.pause();
                    
                    const setId = audioElement.id.replace('clean-audio-', '');
                    const playBtn = document.getElementById(`clean-play-btn-${setId}`);
                    if (playBtn) {
                        playBtn.innerHTML = '<i class="fas fa-play"></i>';
                        playBtn.style.background = 'var(--gradient-primary)';
                    }
                }
            });
        }
        
        function changeCleanVolume(audioId, value) {
            const audio = document.getElementById(audioId);
            if (audio) {
                audio.volume = value / 100;
            }
        }
        
        function updateCleanProgress(setId, currentTime, duration) {
            const progressFill = document.getElementById(`clean-progress-fill-${setId}`);
            const currentTimeElement = document.getElementById(`clean-current-time-${setId}`);
            
            if (duration && duration > 0) {
                const progressPercent = (currentTime / duration) * 100;
                if (progressFill) {
                    progressFill.style.width = progressPercent + '%';
                }
                if (currentTimeElement) {
                    currentTimeElement.textContent = formatTime(currentTime);
                }
            }
        }
        
        function initCleanDuration(setId, duration) {
            const durationElement = document.getElementById(`clean-duration-${setId}`);
            if (durationElement && duration) {
                durationElement.textContent = formatTime(duration);
            }
        }
        
        function onCleanAudioEnded(setId) {
            const playBtn = document.getElementById(`clean-play-btn-${setId}`);
            if (playBtn) {
                playBtn.innerHTML = '<i class="fas fa-play"></i>';
                playBtn.style.background = 'var(--gradient-primary)';
            }
        }
        
        function seekCleanAudio(audioId, setId, event) {
            const audio = document.getElementById(audioId);
            const progressBar = event.currentTarget;
            const rect = progressBar.getBoundingClientRect();
            const clickX = event.clientX - rect.left;
            const percent = clickX / rect.width;
            
            if (audio && audio.duration) {
                audio.currentTime = percent * audio.duration;
                updateCleanProgress(setId, audio.currentTime, audio.duration);
            }
        }
        
        function cleanLikeSet(setId) {
            if (!isUserLoggedIn()) {
                alert('–¢—Ä—è–±–≤–∞ –¥–∞ —Å—Ç–µ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏, –∑–∞ –¥–∞ —Ö–∞—Ä–µ—Å–≤–∞—Ç–µ —Å–µ—Ç–æ–≤–µ!');
                return;
            }
            
            const likeBtn = document.getElementById(`clean-like-btn-${setId}`);
            const likeCountElement = likeBtn.querySelector('.clean-like-count');
            const isCurrentlyLiked = localStorage.getItem(`liked_${setId}`) === 'true';
            
            if (isCurrentlyLiked) {
                siteData.likes[setId] = (siteData.likes[setId] || 1) - 1;
                likeBtn.classList.remove('liked');
                likeBtn.querySelector('i').className = 'far fa-heart';
                localStorage.setItem(`liked_${setId}`, 'false');
            } else {
                siteData.likes[setId] = (siteData.likes[setId] || 0) + 1;
                likeBtn.classList.add('liked');
                likeBtn.querySelector('i').className = 'fas fa-heart';
                localStorage.setItem(`liked_${setId}`, 'true');
                likeBtn.style.transform = 'scale(1.1)';
                setTimeout(() => {
                    likeBtn.style.transform = 'scale(1)';
                }, 200);
            }
            
            if (likeCountElement) {
                likeCountElement.textContent = siteData.likes[setId] || 0;
            }
            
            // –ê–∫—Ç—É–∞–ª–∏–∑–∏—Ä–∞–º–µ –∏ –º–µ—Ç–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è—Ç–∞
            const metaElement = document.querySelector(`#clean-player-${setId} .set-player-clean-meta div:nth-child(3)`);
            if (metaElement) {
                metaElement.innerHTML = `<i class="fas fa-heart"></i> ${siteData.likes[setId] || 0} —Ö–∞—Ä–µ—Å–≤–∞–Ω–∏—è`;
            }
            
            saveLikesToStorage();
            saveDataToStorage();
        }
        
        function cleanDownloadSet(setId) {
            if (!isUserLoggedIn()) {
                alert('–¢—Ä—è–±–≤–∞ –¥–∞ —Å—Ç–µ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏, –∑–∞ –¥–∞ —Å–≤–∞–ª—è—Ç–µ —Å–µ—Ç–æ–≤–µ—Ç–µ!');
                return;
            }
            
            const set = siteData.sets.find(s => s.id === setId);
            if (!set) return;
            
            const downloadUrl = set.downloadUrl || set.url;
            const link = document.createElement('a');
            link.href = downloadUrl;
            link.download = `${set.dj} - ${set.title}.mp3`.replace(/[<>:"/\\|?*]/g, '_');
            link.target = '_blank';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        }
        
        function downloadSet(setId) {
            if (!isUserLoggedIn()) {
                alert('–¢—Ä—è–±–≤–∞ –¥–∞ —Å—Ç–µ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏, –∑–∞ –¥–∞ —Å–≤–∞–ª—è—Ç–µ —Å–µ—Ç–æ–≤–µ—Ç–µ!');
                return;
            }
            
            const set = siteData.sets.find(s => s.id === setId);
            if (!set) return;
            
            const downloadUrl = set.downloadUrl || set.url;
            const link = document.createElement('a');
            link.href = downloadUrl;
            link.download = `${set.dj} - ${set.title}.mp3`.replace(/[<>:"/\\|?*]/g, '_');
            link.target = '_blank';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        }
        
        function likeSet(setId) {
            if (!isUserLoggedIn()) {
                alert('–¢—Ä—è–±–≤–∞ –¥–∞ —Å—Ç–µ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏, –∑–∞ –¥–∞ —Ö–∞—Ä–µ—Å–≤–∞—Ç–µ —Å–µ—Ç–æ–≤–µ!');
                return;
            }
            
            const likeBtn = document.getElementById(`likeBtn_${setId}`);
            const likeCountElement = likeBtn.querySelector('.like-count');
            const isCurrentlyLiked = localStorage.getItem(`liked_${setId}`) === 'true';
            
            if (isCurrentlyLiked) {
                siteData.likes[setId] = (siteData.likes[setId] || 1) - 1;
                likeBtn.classList.remove('liked');
                likeBtn.querySelector('i').className = 'far fa-heart';
                localStorage.setItem(`liked_${setId}`, 'false');
            } else {
                siteData.likes[setId] = (siteData.likes[setId] || 0) + 1;
                likeBtn.classList.add('liked');
                likeBtn.querySelector('i').className = 'fas fa-heart';
                localStorage.setItem(`liked_${setId}`, 'true');
                likeBtn.style.transform = 'scale(1.1)';
                setTimeout(() => {
                    likeBtn.style.transform = 'scale(1)';
                }, 200);
            }
            
            if (likeCountElement) {
                likeCountElement.textContent = siteData.likes[setId] || 0;
            }
            
            saveLikesToStorage();
            saveDataToStorage();
        }
        
        function submitRegistrationForm(event) {
            event.preventDefault();
            
            const username = document.getElementById('regUsername').value;
            const password = document.getElementById('regPassword').value;
            const confirmPassword = document.getElementById('regConfirmPassword').value;
            
            if (password !== confirmPassword) {
                alert('–ü–∞—Ä–æ–ª–∏—Ç–µ –Ω–µ —Å—ä–≤–ø–∞–¥–∞—Ç!');
                return;
            }
            
            if (username.length < 3) {
                alert('–ü–æ—Ç—Ä–µ–±–∏—Ç–µ–ª—Å–∫–æ—Ç–æ –∏–º–µ —Ç—Ä—è–±–≤–∞ –¥–∞ –±—ä–¥–µ –ø–æ–Ω–µ 3 —Å–∏–º–≤–æ–ª–∞!');
                return;
            }
            
            if (password.length < 6) {
                alert('–ü–∞—Ä–æ–ª–∞—Ç–∞ —Ç—Ä—è–±–≤–∞ –¥–∞ –±—ä–¥–µ –ø–æ–Ω–µ 6 —Å–∏–º–≤–æ–ª–∞!');
                return;
            }
            
            const existingUser = siteData.users.find(user => user.username === username);
            if (existingUser) {
                alert('–¢–æ–≤–∞ –ø–æ—Ç—Ä–µ–±–∏—Ç–µ–ª—Å–∫–æ –∏–º–µ –≤–µ—á–µ —Å—ä—â–µ—Å—Ç–≤—É–≤–∞!');
                return;
            }
            
            const newUser = {
                id: siteData.users.length + 1,
                username: username,
                password: password,
                registrationDate: new Date().toISOString(),
                role: 'user',
                likes: 0
            };
            
            siteData.users.push(newUser);
            currentUser = newUser;
            localStorage.setItem('currentUser', JSON.stringify(newUser));
            updateNavigationForLoggedInUser();
            saveDataToStorage();
            
            document.getElementById('registrationForm').reset();
            checkSetUploadAccess();
            loadCleanSets();
            
            alert('–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è—Ç–∞ –µ —É—Å–ø–µ—à–Ω–∞! –í–µ—á–µ —Å—Ç–µ –≤–ª–µ–∑–ª–∏ –≤ —Å–∏—Å—Ç–µ–º–∞—Ç–∞ –∏ –∏–º–∞—Ç–µ –¥–æ—Å—Ç—ä–ø –¥–æ –≤—Å–∏—á–∫–∏ —Ñ—É–Ω–∫—Ü–∏–∏.');
            showSection('profile');
        }
        
        function showLoginForm() {
            const username = prompt('–í—ä–≤–µ–¥–µ—Ç–µ –ø–æ—Ç—Ä–µ–±–∏—Ç–µ–ª—Å–∫–æ –∏–º–µ:');
            if (!username) return;
            
            const password = prompt('–í—ä–≤–µ–¥–µ—Ç–µ –ø–∞—Ä–æ–ª–∞:');
            if (!password) return;
            
            if (loginUser(username, password)) {
                alert(`–î–æ–±—Ä–µ –¥–æ—à–ª–∏, ${username}!`);
                checkSetUploadAccess();
                loadCleanSets();
                showSection('profile');
            } else {
                alert('–ì—Ä–µ—à–Ω–æ –ø–æ—Ç—Ä–µ–±–∏—Ç–µ–ª—Å–∫–æ –∏–º–µ –∏–ª–∏ –ø–∞—Ä–æ–ª–∞!');
            }
        }
        
        function playSet(setId) {
            const set = siteData.sets.find(s => s.id === setId);
            if (!set) return;
            
            const modal = document.getElementById('setPlayerModal');
            const title = document.getElementById('setPlayerTitle');
            const audioPlayer = document.getElementById('setAudioPlayer');
            const info = document.getElementById('setPlayerInfo');
            
            title.textContent = `${set.dj} - ${set.title}`;
            audioPlayer.src = set.url;
            info.innerHTML = `
                <p><strong>–°—Ç–∏–ª:</strong> ${set.style}</p>
                <p><strong>–ü—Ä–æ–¥—ä–ª–∂–∏—Ç–µ–ª–Ω–æ—Å—Ç:</strong> ${set.duration} –º–∏–Ω—É—Ç–∏</p>
                <p><strong>–î–∞—Ç–∞:</strong> ${set.date}</p>
                ${set.description ? `<p><strong>–û–ø–∏—Å–∞–Ω–∏–µ:</strong> ${set.description}</p>` : ''}
            `;
            
            modal.classList.add('active');
        }
        
        function closeSetPlayer() {
            const modal = document.getElementById('setPlayerModal');
            const audioPlayer = document.getElementById('setAudioPlayer');
            
            audioPlayer.pause();
            audioPlayer.src = '';
            modal.classList.remove('active');
        }
        
        function initializeAudioPlayer() {
            const audio = document.getElementById('radioStream');
            const playBtn = document.getElementById('playBtn');
            const volumeSlider = document.getElementById('volume');
            
            if (playBtn) {
                playBtn.addEventListener('click', () => {
                    if (audio.paused) {
                        audio.play();
                        playBtn.classList.replace('fa-play-circle', 'fa-pause-circle');
                    } else {
                        audio.pause();
                        playBtn.classList.replace('fa-pause-circle', 'fa-play-circle');
                    }
                });
            }
            
            if (volumeSlider) {
                volumeSlider.addEventListener('input', (e) => {
                    audio.volume = e.target.value;
                });
            }
        }
        
        function startListenerCounter() {
            setInterval(() => {
                let change = Math.floor(Math.random() * 10) - 4;
                siteData.listeners = Math.max(1000, siteData.listeners + change);
                const listenerElement = document.getElementById('publicListenerCount');
                if (listenerElement) {
                    listenerElement.textContent = siteData.listeners.toLocaleString();
                }
            }, 3000);
        }
        
        function openAdminPanel() {
            document.getElementById('passwordModal').classList.add('active');
            document.getElementById('adminPassword').focus();
        }
        
        function checkAdminPassword() {
            const password = document.getElementById('adminPassword').value;
            const errorElement = document.getElementById('passwordError');
            
            if (password === "admin123") {
                document.getElementById('passwordModal').classList.remove('active');
                document.getElementById('adminPassword').value = '';
                errorElement.style.display = 'none';
                
                document.getElementById('main-content').style.display = 'none';
                document.getElementById('admin-panel').style.display = 'block';
                loadAdminData();
                document.title = "–ê–¥–º–∏–Ω –ü–∞–Ω–µ–ª - SOUNDOURA";
            } else {
                errorElement.style.display = 'block';
                document.getElementById('adminPassword').value = '';
                document.getElementById('adminPassword').focus();
            }
        }
        
        function exitAdminPanel() {
            document.getElementById('admin-panel').style.display = 'none';
            document.getElementById('main-content').style.display = 'block';
            document.title = "SOUNDOURA - Web3 –∏–∑–¥–∞–Ω–∏–µ";
            initializeSite();
        }
        
        function switchAdminTab(tabId) {
            document.querySelectorAll('.admin-tab-section').forEach(section => {
                section.classList.remove('active');
            });
            
            document.querySelectorAll('.admin-tab').forEach(tab => {
                tab.classList.remove('active');
            });
            
            document.getElementById('admin-' + tabId).classList.add('active');
            event.target.classList.add('active');
            
            if (tabId === 'pending-sets') {
                loadPendingSets();
            } else if (tabId === 'sets') {
                loadSetsList();
            } else if (tabId === 'tracks') {
                loadTracksList();
            } else if (tabId === 'contacts') {
                loadContacts();
            } else if (tabId === 'users') {
                loadUsersList();
            }
        }
        
        function loadAdminData() {
            loadPendingSets();
            loadSetsList();
            loadTracksList();
            loadContacts();
            loadUsersList();
            
            const today = new Date();
            const formattedDate = today.toLocaleDateString('bg-BG');
            document.getElementById('new-set-date').value = formattedDate;
        }
        
        function loadPendingSets() {
            const pendingContainer = document.getElementById('pending-sets-container');
            const pendingCountBadge = document.getElementById('pendingCountBadge');
            
            pendingContainer.innerHTML = '';
            if (pendingCountBadge) {
                pendingCountBadge.textContent = siteData.pendingSets.length;
            }
            
            if (siteData.pendingSets.length === 0) {
                pendingContainer.innerHTML = '<p style="text-align:center; color:rgba(255,255,255,0.5); padding:40px;">–ù—è–º–∞ —Å–µ—Ç–æ–≤–µ, —á–∞–∫–∞—â–∏ –æ–¥–æ–±—Ä–µ–Ω–∏–µ.</p>';
                return;
            }
            
            siteData.pendingSets.forEach((set, index) => {
                const pendingItem = document.createElement('div');
                pendingItem.className = 'pending-set-item';
                pendingItem.style.cssText = 'background:rgba(255,255,255,0.02); border-radius:10px; border:1px solid rgba(255,0,255,0.3); padding:20px; margin-bottom:15px; max-width:800px; margin-left:auto; margin-right:auto;';
                
                const submittedDate = new Date(set.submittedDate);
                const formattedDate = submittedDate.toLocaleDateString('bg-BG') + ' ' + submittedDate.toLocaleTimeString('bg-BG', { hour: '2-digit', minute: '2-digit' });
                
                pendingItem.innerHTML = `
                    <div style="display:flex; justify-content:space-between; align-items:center; margin-bottom:10px; flex-wrap:wrap; gap:10px;">
                        <div style="font-size:1.1rem; color:#fff; font-weight:700; flex:1; min-width:200px; text-align:left;">${set.title}</div>
                        <div style="color:rgba(255,255,255,0.5); font-size:0.8rem; text-align:right;">–ò–∑–ø—Ä–∞—Ç–µ–Ω–æ: ${formattedDate}</div>
                    </div>
                    <div style="margin-bottom:15px; text-align:left;">
                        <p style="margin:5px 0; color:rgba(255,255,255,0.7); font-size:0.9rem;"><strong>–°—Ç–∏–ª:</strong> ${set.style}</p>
                        <p style="margin:5px 0; color:rgba(255,255,255,0.7); font-size:0.9rem;"><strong>DJ:</strong> ${set.dj}</p>
                        <p style="margin:5px 0; color:rgba(255,255,255,0.7); font-size:0.9rem;"><strong>–ü—Ä–æ–¥—ä–ª–∂–∏—Ç–µ–ª–Ω–æ—Å—Ç:</strong> ${set.duration} –º–∏–Ω—É—Ç–∏</p>
                        <p style="margin:5px 0; color:rgba(255,255,255,0.7); font-size:0.9rem;"><strong>–õ–∏–Ω–∫:</strong> <a href="${set.url}" target="_blank" style="color:#ff00ff;">${set.url.substring(0, 50)}${set.url.length > 50 ? '...' : ''}</a></p>
                        ${set.description ? `<p style="margin:5px 0; color:rgba(255,255,255,0.7); font-size:0.9rem;"><strong>–û–ø–∏—Å–∞–Ω–∏–µ:</strong> ${set.description}</p>` : ''}
                    </div>
                    <div style="display:flex; gap:10px; justify-content:flex-end; flex-wrap:wrap;">
                        <button class="admin-btn btn-small" onclick="approveSet(${index})" style="background:#00cc66;">
                            <i class="fas fa-check"></i> –û–¥–æ–±—Ä–∏
                        </button>
                        <button class="admin-btn btn-small" onclick="rejectSet(${index})" style="background:#ff4444;">
                            <i class="fas fa-times"></i> –û—Ç—Ö–≤—ä—Ä–ª–∏
                        </button>
                    </div>
                `;
                
                pendingContainer.appendChild(pendingItem);
            });
        }
        
        function loadTracksList() {
            const tracksContainer = document.getElementById('admin-tracks');
            if (tracksContainer) {
                const content = tracksContainer.querySelector('.admin-form-section');
                if (content) {
                    content.innerHTML = `
                        <div class="admin-form-group">
                            <p style="color:rgba(255,255,255,0.7); text-align:center; padding:40px 20px;">–¢–∞–∑–∏ —Å–µ–∫—Ü–∏—è –µ –æ—Å—Ç–∞–≤–µ–Ω–∞ –ø—Ä–∞–∑–Ω–∞ –∑–∞ –±—ä–¥–µ—â–∞ —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª–Ω–æ—Å—Ç –∑–∞ —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –Ω–∞ –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª–Ω–∏ —Ç—Ä–∞–∫–æ–≤–µ.</p>
                            <div style="text-align:center; margin-top:20px;">
                                <i class="fas fa-wave-square" style="font-size:3rem; color:var(--primary-neon);"></i>
                                <p style="margin-top:10px; color:rgba(255,255,255,0.5);">–í –±—ä–¥–µ—â–µ —Ç—É–∫ —â–µ –º–æ–∂–µ –¥–∞ –¥–æ–±–∞–≤—è—Ç–µ –∏ —É–ø—Ä–∞–≤–ª—è–≤–∞—Ç–µ –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª–Ω–∏ —Ç—Ä–∞–∫–æ–≤–µ.</p>
                            </div>
                        </div>
                    `;
                }
            }
        }
        
        function loadUsersList() {
            const usersList = document.getElementById('users-list');
            usersList.innerHTML = '';
            
            if (siteData.users.length === 0) {
                usersList.innerHTML = '<p style="text-align:center; color:rgba(255,255,255,0.5);">–í—Å–µ –æ—â–µ –Ω—è–º–∞ —Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω–∏ –ø–æ—Ç—Ä–µ–±–∏—Ç–µ–ª–∏.</p>';
                return;
            }
            
            siteData.users.forEach((user, index) => {
                const userItem = document.createElement('div');
                userItem.className = 'home-news-list-item';
                userItem.style.cssText = 'display:flex; justify-content:space-between; align-items:center; padding:15px; background:rgba(255,255,255,0.02); border-radius:10px; border:1px solid rgba(255,255,255,0.05); margin-bottom:10px; text-align:left; max-width:800px; margin-left:auto; margin-right:auto;';
                
                const regDate = new Date(user.registrationDate);
                const formattedDate = regDate.toLocaleDateString('bg-BG');
                
                userItem.innerHTML = `
                    <div>
                        <h4 style="margin:0; color:#fff; font-size:1rem; text-align:left;">${user.username}</h4>
                        <p style="margin:5px 0 0 0; color:rgba(255,255,255,0.5); font-size:0.8rem; text-align:left;">–†–µ–≥–∏—Å—Ç—Ä–∏—Ä–∞–Ω: ${formattedDate} | –†–æ–ª—è: ${user.role}</p>
                    </div>
                    <div style="display:flex; gap:10px;">
                        <button class="admin-btn btn-small" onclick="removeUser(${index})" style="background:#ff4444;">
                            <i class="fas fa-trash"></i> –ü—Ä–µ–º–∞—Ö–Ω–∏
                        </button>
                    </div>
                `;
                
                usersList.appendChild(userItem);
            });
        }
        
        function loadSetsList() {
            const setsList = document.getElementById('sets-list');
            setsList.innerHTML = '';
            
            if (siteData.sets.length === 0) {
                setsList.innerHTML = '<p style="text-align:center; color:rgba(255,255,255,0.5);">–í—Å–µ –æ—â–µ –Ω—è–º–∞ –¥–æ–±–∞–≤–µ–Ω–∏ —Å–µ—Ç–æ–≤–µ –≤ –∞—Ä—Ö–∏–≤–∞.</p>';
                return;
            }
            
            siteData.sets.forEach((set, index) => {
                const setItem = document.createElement('div');
                setItem.className = 'home-news-list-item';
                setItem.style.cssText = 'display:flex; justify-content:space-between; align-items:center; padding:15px; background:rgba(255,255,255,0.02); border-radius:10px; border:1px solid rgba(255,255,255,0.05); margin-bottom:10px; text-align:left; max-width:800px; margin-left:auto;margin-right:auto;';
                
                const likeCount = siteData.likes[set.id] || 0;
                
                setItem.innerHTML = `
                    <div>
                        <h4 style="margin:0; color:#fff; font-size:1rem; text-align:left;">${set.dj} - ${set.title}</h4>
                        <p style="margin:5px 0 0 0; color:rgba(255,255,255,0.5); font-size:0.8rem; text-align:left;">${set.date} | ${set.style} | ${set.duration} –º–∏–Ω. | –õ–∞–π–∫–æ–≤–µ: ${likeCount}</p>
                    </div>
                    <div style="display:flex; gap:10px;">
                        <button class="admin-btn btn-small" onclick="editSet(${index})" style="background:#ffcc00;">
                            <i class="fas fa-edit"></i> –†–µ–¥–∞–∫—Ç–∏—Ä–∞–π
                        </button>
                        <button class="admin-btn btn-small" onclick="removeSet(${index})" style="background:#ff4444;">
                            <i class="fas fa-trash"></i> –ü—Ä–µ–º–∞—Ö–Ω–∏
                        </button>
                    </div>
                `;
                
                setsList.appendChild(setItem);
            });
        }
        
        function loadContacts() {
            document.getElementById('contact-text').value = siteData.contacts.text;
            document.getElementById('contact-address').value = siteData.contacts.address;
            document.getElementById('contact-phone').value = siteData.contacts.phone;
            document.getElementById('contact-email').value = siteData.contacts.email;
        }
        
        function editSet(index) {
            const set = siteData.sets[index];
            if (!set) return;
            
            currentEditingSetIndex = index;
            
            document.getElementById('edit-set-dj').value = set.dj;
            document.getElementById('edit-set-title').value = set.title;
            document.getElementById('edit-set-style').value = set.style;
            document.getElementById('edit-set-duration').value = set.duration;
            document.getElementById('edit-set-url').value = set.url;
            document.getElementById('edit-set-download-url').value = set.downloadUrl || '';
            document.getElementById('edit-set-file-size').value = set.fileSize || '';
            document.getElementById('edit-set-date').value = set.date;
            document.getElementById('edit-set-description').value = set.description || '';
            
            document.getElementById('editSetModal').classList.add('active');
        }
        
        function saveEditedSet() {
            if (currentEditingSetIndex === -1) return;
            
            const set = siteData.sets[currentEditingSetIndex];
            if (!set) return;
            
            set.dj = document.getElementById('edit-set-dj').value;
            set.title = document.getElementById('edit-set-title').value;
            set.style = document.getElementById('edit-set-style').value;
            set.duration = parseInt(document.getElementById('edit-set-duration').value);
            set.url = document.getElementById('edit-set-url').value;
            set.downloadUrl = document.getElementById('edit-set-download-url').value || null;
            set.fileSize = document.getElementById('edit-set-file-size').value || null;
            set.date = document.getElementById('edit-set-date').value;
            set.description = document.getElementById('edit-set-description').value || '';
            
            saveAllChanges();
            closeEditSetModal();
            loadSetsList();
            loadCleanSets();
            loadLatestAlbums();
            alert('–ü—Ä–æ–º–µ–Ω–∏—Ç–µ —Å–∞ –∑–∞–ø–∞–∑–µ–Ω–∏ —É—Å–ø–µ—à–Ω–æ!');
        }
        
        function closeEditSetModal() {
            document.getElementById('editSetModal').classList.remove('active');
            currentEditingSetIndex = -1;
            
            document.getElementById('edit-set-dj').value = '';
            document.getElementById('edit-set-title').value = '';
            document.getElementById('edit-set-style').value = '';
            document.getElementById('edit-set-duration').value = '';
            document.getElementById('edit-set-url').value = '';
            document.getElementById('edit-set-download-url').value = '';
            document.getElementById('edit-set-file-size').value = '';
            document.getElementById('edit-set-date').value = '';
            document.getElementById('edit-set-description').value = '';
        }
        
        function removeUser(index) {
            if (confirm('–°–∏–≥—É—Ä–Ω–∏ –ª–∏ —Å—Ç–µ, —á–µ –∏—Å–∫–∞—Ç–µ –¥–∞ –ø—Ä–µ–º–∞—Ö–Ω–µ—Ç–µ —Ç–æ–∑–∏ –ø–æ—Ç—Ä–µ–±–∏—Ç–µ–ª?')) {
                siteData.users.splice(index, 1);
                loadUsersList();
                saveAllChanges();
                alert('–ü–æ—Ç—Ä–µ–±–∏—Ç–µ–ª—è—Ç –µ –ø—Ä–µ–º–∞—Ö–Ω–∞—Ç —É—Å–ø–µ—à–Ω–æ!');
            }
        }
        
        function approveSet(index) {
            if (confirm('–°–∏–≥—É—Ä–Ω–∏ –ª–∏ —Å—Ç–µ, —á–µ –∏—Å–∫–∞—Ç–µ –¥–∞ –æ–¥–æ–±—Ä–∏—Ç–µ —Ç–æ–∑–∏ —Å–µ—Ç?')) {
                const pendingSet = siteData.pendingSets[index];
                const approvedSet = {
                    ...pendingSet,
                    id: siteData.sets.length + 1,
                    status: 'approved',
                    approvedDate: new Date().toISOString()
                };
                
                siteData.sets.unshift(approvedSet);
                siteData.pendingSets.splice(index, 1);
                
                loadPendingSets();
                loadLatestAlbums();
                loadCleanSets();
                saveAllChanges();
                alert('–°–µ—Ç—ä—Ç –µ –æ–¥–æ–±—Ä–µ–Ω —É—Å–ø–µ—à–Ω–æ –∏ –≤–µ—á–µ —Å–µ –ø–æ–∫–∞–∑–≤–∞ –≤ –∞—Ä—Ö–∏–≤–∞ –∏ –Ω–∞ –Ω–∞—á–∞–ª–Ω–∞—Ç–∞ —Å—Ç—Ä–∞–Ω–∏—Ü–∞!');
            }
        }
        
        function rejectSet(index) {
            if (confirm('–°–∏–≥—É—Ä–Ω–∏ –ª–∏ —Å—Ç–µ, —á–µ –∏—Å–∫–∞—Ç–µ –¥–∞ –æ—Ç—Ö–≤—ä—Ä–ª–∏—Ç–µ —Ç–æ–∑–∏ —Å–µ—Ç?')) {
                siteData.pendingSets.splice(index, 1);
                loadPendingSets();
                saveAllChanges();
                alert('–°–µ—Ç—ä—Ç –µ –æ—Ç—Ö–≤—ä—Ä–ª–µ–Ω —É—Å–ø–µ—à–Ω–æ!');
            }
        }
        
        function analyzeAudioFile() {
            const urlInput = document.getElementById('auto-generate-url');
            const url = urlInput.value.trim();
            const statusElement = document.getElementById('auto-generate-status');
            const previewElement = document.getElementById('auto-generate-preview');
            
            if (!url) {
                statusElement.textContent = '–ú–æ–ª—è, –≤—ä–≤–µ–¥–µ—Ç–µ –≤–∞–ª–∏–¥–µ–Ω MP3 –ª–∏–Ω–∫!';
                statusElement.className = 'auto-generate-status error';
                return;
            }
            
            if (!url.match(/\.(mp3|wav|ogg|m4a|aac)$/i)) {
                statusElement.textContent = 'URL-–∞ —Ç—Ä—è–±–≤–∞ –¥–∞ –∑–∞–≤—ä—Ä—à–≤–∞ —Å .mp3, .wav, .ogg, .m4a –∏–ª–∏ .aac';
                statusElement.className = 'auto-generate-status error';
                return;
            }
            
            statusElement.textContent = '–ê–Ω–∞–ª–∏–∑–∏—Ä–∞–Ω–µ –Ω–∞ –∞—É–¥–∏–æ —Ñ–∞–π–ª–∞... –ú–æ–ª—è –∏–∑—á–∞–∫–∞–π—Ç–µ!';
            statusElement.className = 'auto-generate-status loading';
            previewElement.style.display = 'none';
            
            setTimeout(() => {
                simulateAudioAnalysis(url);
            }, 1500);
        }
        
        function simulateAudioAnalysis(url) {
            const statusElement = document.getElementById('auto-generate-status');
            const previewElement = document.getElementById('auto-generate-preview');
            
            try {
                const fileName = url.split('/').pop().split('?')[0];
                const fileExtension = fileName.split('.').pop().toUpperCase();
                
                const fileSizes = ['45 MB', '68 MB', '92 MB', '120 MB', '156 MB', '210 MB'];
                const durations = [45, 60, 75, 90, 105, 120];
                const djNames = ['DJ Pulse', 'Electro Master', 'Bass Hunter', 'Tech Vibes', 'Melodic Flow', 'Deep Soul'];
                const trackTitles = [
                    'Summer Vibes Mix',
                    'Night Drive Sessions',
                    'Underground Beats',
                    'Sunset Grooves',
                    'Midnight Energy',
                    'Urban Rhythms'
                ];
                
                const randomSize = fileSizes[Math.floor(Math.random() * fileSizes.length)];
                const randomDuration = durations[Math.floor(Math.random() * durations.length)];
                const randomDJ = djNames[Math.floor(Math.random() * djNames.length)];
                const randomTitle = trackTitles[Math.floor(Math.random() * trackTitles.length)];
                
                audioAnalysisData = {
                    url: url,
                    fileName: fileName,
                    size: randomSize,
                    duration: randomDuration,
                    format: fileExtension,
                    djName: randomDJ,
                    trackTitle: randomTitle
                };
                
                document.getElementById('preview-filename').textContent = fileName;
                document.getElementById('preview-size').textContent = randomSize;
                document.getElementById('preview-duration').textContent = randomDuration + ' –º–∏–Ω—É—Ç–∏';
                document.getElementById('preview-format').textContent = fileExtension;
                
                statusElement.textContent = '–ê–Ω–∞–ª–∏–∑—ä—Ç –µ —É—Å–ø–µ—à–µ–Ω! –ú–æ–∂–µ—Ç–µ –¥–∞ –∏–∑–ø–æ–ª–∑–≤–∞—Ç–µ –≥–µ–Ω–µ—Ä–∏—Ä–∞–Ω–∏—Ç–µ –¥–∞–Ω–Ω–∏.';
                statusElement.className = 'auto-generate-status success';
                previewElement.style.display = 'block';
                
            } catch (error) {
                statusElement.textContent = '–ì—Ä–µ—à–∫–∞ –ø—Ä–∏ –∞–Ω–∞–ª–∏–∑ –Ω–∞ —Ñ–∞–π–ª–∞. –ú–æ–ª—è, –ø—Ä–æ–≤–µ—Ä–µ—Ç–µ URL-–∞.';
                statusElement.className = 'auto-generate-status error';
                console.error('Audio analysis error:', error);
            }
        }
        
        function useGeneratedData() {
            if (!audioAnalysisData) {
                alert('–ú–æ–ª—è, –ø—ä—Ä–≤–æ –∞–Ω–∞–ª–∏–∑–∏—Ä–∞–π—Ç–µ –∞—É–¥–∏–æ —Ñ–∞–π–ª–∞!');
                return;
            }
            
            const fileNameWithoutExt = audioAnalysisData.fileName.replace(/\.[^/.]+$/, "");
            const generatedDJName = generateDJNameFromFileName(fileNameWithoutExt);
            
            document.getElementById('new-set-dj').value = generatedDJName;
            document.getElementById('new-set-title').value = fileNameWithoutExt.replace(/[_-]/g, ' ');
            document.getElementById('new-set-url').value = audioAnalysisData.url;
            document.getElementById('new-set-download-url').value = audioAnalysisData.url;
            document.getElementById('new-set-file-size').value = audioAnalysisData.size;
            document.getElementById('new-set-duration').value = audioAnalysisData.duration;
            
            const style = guessMusicStyle(fileNameWithoutExt);
            document.getElementById('new-set-style').value = style;
            
            const description = generateDescription(generatedDJName, fileNameWithoutExt, audioAnalysisData.duration);
            document.getElementById('new-set-description').value = description;
            
            const statusElement = document.getElementById('auto-generate-status');
            statusElement.textContent = '–î–∞–Ω–Ω–∏—Ç–µ —Å–∞ –∞–≤—Ç–æ–º–∞—Ç–∏—á–Ω–æ –ø–æ–ø—ä–ª–Ω–µ–Ω–∏ –≤—ä–≤ —Ñ–æ—Ä–º–∞—Ç–∞!';
            statusElement.className = 'auto-generate-status success';
            
            document.querySelector('.admin-form-section').scrollIntoView({ behavior: 'smooth' });
        }
        
        function generateDJNameFromFileName(fileName) {
            const djPrefixes = ['DJ', 'MC', 'Pro', 'Deep', 'Tech', 'Urban'];
            const djSuffixes = ['Wave', 'Flow', 'Beat', 'Pulse', 'Rhythm', 'Vibe', 'Soul', 'Mind'];
            
            const words = fileName.split(/[_\-\s]+/);
            let baseName = '';
            
            if (words.length >= 2) {
                baseName = words.slice(0, 2).map(word => 
                    word.charAt(0).toUpperCase() + word.slice(1).toLowerCase()
                ).join(' ');
            } else {
                baseName = fileName.charAt(0).toUpperCase() + fileName.slice(1).toLowerCase();
            }
            
            const addPrefix = Math.random() > 0.5;
            if (addPrefix) {
                const prefix = djPrefixes[Math.floor(Math.random() * djPrefixes.length)];
                return prefix + ' ' + baseName;
            } else {
                const suffix = djSuffixes[Math.floor(Math.random() * djSuffixes.length)];
                return baseName + ' ' + suffix;
            }
        }
        
        function guessMusicStyle(fileName) {
            const fileNameLower = fileName.toLowerCase();
            
            if (fileNameLower.includes('deep') || fileNameLower.includes('soul') || fileNameLower.includes('chill')) {
                return 'Deep House';
            } else if (fileNameLower.includes('tech') || fileNameLower.includes('hard') || fileNameLower.includes('drive')) {
                return 'Tech House';
            } else if (fileNameLower.includes('techno') || fileNameLower.includes('industrial') || fileNameLower.includes('dark')) {
                return 'Techno';
            } else if (fileNameLower.includes('prog') || fileNameLower.includes('progressive') || fileNameLower.includes('journey')) {
                return 'Progressive';
            } else if (fileNameLower.includes('minimal') || fileNameLower.includes('micro') || fileNameLower.includes('clean')) {
                return 'Minimal';
            } else if (fileNameLower.includes('melodic') || fileNameLower.includes('emotional') || fileNameLower.includes('feeling')) {
                return 'Melodic';
            } else {
                const styles = ['Deep House', 'Tech House', 'Techno', 'Progressive', 'Minimal', 'Melodic'];
                return styles[Math.floor(Math.random() * styles.length)];
            }
        }
        
        function generateDescription(djName, title, duration) {
            const descriptions = [
                `${djName} –ø—Ä–µ–¥—Å—Ç–∞–≤—è "${title}" - ${duration}-–º–∏–Ω—É—Ç–µ–Ω –º–∏–∫—Å, –ø–µ—Ä—Ñ–µ–∫—Ç–µ–Ω –∑–∞ –Ω–æ—â–Ω–æ –∫–∞—Ä–∞–Ω–µ –∏–ª–∏ –ø–∞—Ä—Ç–∏.`,
                `–°–ø–µ—Ü–∏–∞–ª–Ω–∞ —Å–µ—Å–∏—è –æ—Ç ${djName}. ${duration} –º–∏–Ω—É—Ç–∏ —á–∏—Å—Ç –∑–≤—É–∫ –∏ –Ω–µ–ø–æ–≤—Ç–æ—Ä–∏–º–∏ –≤–∏–±—Ä–∞—Ü–∏–∏.`,
                `–ï–∫—Å–∫–ª—É–∑–∏–≤–µ–Ω —Å–µ—Ç –æ—Ç ${djName}, —Å—ä–∑–¥–∞–¥–µ–Ω —Å–ø–µ—Ü–∏–∞–ª–Ω–æ –∑–∞ SOUNDOURA. –ù–∞—Å–ª–∞–¥–µ—Ç–µ —Å–µ –Ω–∞ ${duration} –º–∏–Ω—É—Ç–∏ –º—É–∑–∏–∫–∞.`,
                `${title} –æ—Ç ${djName}. ${duration}-–º–∏–Ω—É—Ç–Ω–æ –ø—ä—Ç—É–≤–∞–Ω–µ –≤ —Å–≤–µ—Ç–∞ –Ω–∞ –µ–ª–µ–∫—Ç—Ä–æ–Ω–Ω–∞—Ç–∞ –º—É–∑–∏–∫–∞.`,
                `–°–≤–µ–∂ –º–∏–∫—Å –æ—Ç ${djName}. ${duration} –º–∏–Ω—É—Ç–∏ –ø–æ–¥–±—Ä–∞–Ω–∏ —Ç—Ä–∞–∫–æ–≤–µ –∏ –ø–ª–∞–≤–Ω–∏ –ø—Ä–µ—Ö–æ–¥–∏.`
            ];
            
            return descriptions[Math.floor(Math.random() * descriptions.length)];
        }
        
        function resetAutoGenerate() {
            document.getElementById('auto-generate-url').value = '';
            document.getElementById('auto-generate-status').textContent = '';
            document.getElementById('auto-generate-status').className = 'auto-generate-status';
            document.getElementById('auto-generate-preview').style.display = 'none';
            audioAnalysisData = null;
        }
        
        function addSet() {
            const dj = document.getElementById('new-set-dj').value;
            const title = document.getElementById('new-set-title').value;
            const style = document.getElementById('new-set-style').value;
            const duration = document.getElementById('new-set-duration').value;
            const url = document.getElementById('new-set-url').value;
            const downloadUrl = document.getElementById('new-set-download-url').value;
            const fileSize = document.getElementById('new-set-file-size').value;
            const dateInput = document.getElementById('new-set-date');
            const date = dateInput.value;
            const description = document.getElementById('new-set-description').value;
            
            if (!dj || !title || !style || !duration || !url || !date) {
                alert('–ú–æ–ª—è, –ø–æ–ø—ä–ª–Ω–µ—Ç–µ –≤—Å–∏—á–∫–∏ –∑–∞–¥—ä–ª–∂–∏—Ç–µ–ª–Ω–∏ –ø–æ–ª–µ—Ç–∞!');
                return;
            }
            
            const newSet = {
                id: siteData.sets.length + 1,
                dj: dj,
                title: title,
                style: style,
                duration: parseInt(duration),
                date: date,
                description: description || '',
                url: url,
                downloadUrl: downloadUrl || null,
                fileSize: fileSize || null
            };
            
            siteData.sets.unshift(newSet);
            
            document.getElementById('new-set-dj').value = '';
            document.getElementById('new-set-title').value = '';
            document.getElementById('new-set-style').value = '';
            document.getElementById('new-set-duration').value = '';
            document.getElementById('new-set-url').value = '';
            document.getElementById('new-set-download-url').value = '';
            document.getElementById('new-set-file-size').value = '';
            
            const today = new Date();
            const formattedDate = today.toLocaleDateString('bg-BG');
            dateInput.value = formattedDate;
            
            document.getElementById('new-set-description').value = '';
            
            loadSetsList();
            loadLatestAlbums();
            loadCleanSets();
            saveAllChanges();
            
            alert('–°–µ—Ç—ä—Ç –µ –¥–æ–±–∞–≤–µ–Ω —É—Å–ø–µ—à–Ω–æ –≤ –∞—Ä—Ö–∏–≤–∞! –õ–∏–Ω–∫—ä—Ç —â–µ —Å–µ –ø–æ–∫–∞–∂–µ –∫–∞—Ç–æ MP3 –ø–ª–µ–π—ä—Ä.');
        }
        
        function removeSet(index) {
            if (confirm('–°–∏–≥—É—Ä–Ω–∏ –ª–∏ —Å—Ç–µ, —á–µ –∏—Å–∫–∞—Ç–µ –¥–∞ –ø—Ä–µ–º–∞—Ö–Ω–µ—Ç–µ —Ç–æ–∑–∏ —Å–µ—Ç?')) {
                siteData.sets.splice(index, 1);
                loadSetsList();
                loadLatestAlbums();
                loadCleanSets();
                saveAllChanges();
                alert('–°–µ—Ç—ä—Ç –µ –ø—Ä–µ–º–∞—Ö–Ω–∞—Ç —É—Å–ø–µ—à–Ω–æ!');
            }
        }
        
        function saveContacts() {
            siteData.contacts = {
                text: document.getElementById('contact-text').value,
                address: document.getElementById('contact-address').value,
                phone: document.getElementById('contact-phone').value,
                email: document.getElementById('contact-email').value
            };
            
            saveAllChanges();
            alert('–ö–æ–Ω—Ç–∞–∫—Ç–∏—Ç–µ —Å–∞ –∑–∞–ø–∞–∑–µ–Ω–∏ —É—Å–ø–µ—à–Ω–æ!');
        }
        
        function saveAllChanges() {
            localStorage.setItem('soundouraData', JSON.stringify(siteData));
            saveLikesToStorage();
            
            loadLatestAlbums();
            loadCleanSets();
            
            const successMsg = document.createElement('div');
            successMsg.innerHTML = '<div style="position:fixed; top:20px; right:20px; background:#00cc66; color:white; padding:15px 25px; border-radius:10px; z-index:10000; box-shadow:0 5px 15px rgba(0,0,0,0.3);"><i class="fas fa-check-circle"></i> –ü—Ä–æ–º–µ–Ω–∏—Ç–µ —Å–∞ –∑–∞–ø–∞–∑–µ–Ω–∏ —É—Å–ø–µ—à–Ω–æ!</div>';
            document.body.appendChild(successMsg);
            setTimeout(() => successMsg.remove(), 3000);
        }
        
        window.addEventListener('storage', function(e) {
            if (e.key === 'soundouraData') {
                loadDataFromStorage();
                initializeSite();
            }
            if (e.key === 'soundouraLikes') {
                loadLikesFromStorage();
                loadCleanSets();
            }
        });
        
        document.addEventListener('DOMContentLoaded', function() {
            const splashScreen = document.getElementById('splash-screen');
            const mainContent = document.getElementById('main-content');
            
            setTimeout(function() {
                splashScreen.style.display = 'none';
                mainContent.style.display = 'block';
                initializeSite();
                initializeAudioPlayer();
                startListenerCounter();
            }, 4500);
            
            document.getElementById('adminPassword').addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    checkAdminPassword();
                }
            });
        });
    </script>
